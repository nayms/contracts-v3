{
  "filter_paths": "lib",
  "legacy_ast": false,
  "solc_remaps": [
    "ds-test/=lib/ds-test/src/",
    "forge-std/=lib/forge-std/src/",
    "@openzeppelin/=lib/oz/",
    "src/=./src/",
    "test/=./test/",
    "script/=./script/"
  ],
  "detectors_to_exclude": "solc-version,assembly-usage,naming-convention,low-level-calls",
  "exclude_informational": false,
  "exclude_low": false,
  "solc_disable_warnings": false,
  "ignore_compile": true,
  "sarif": "results.sarif"
}
